{
  "name": "antd-jest-demo",
  "version": "1.0.0",
  "description": "A demo for using Jest with Ant Design and some other extras",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 9900 -c .storybook",
    "lint": "eslint __tests__/**/*.js __tests__/**/*.jsx src/**/*.js src/**/*.jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orangecms/antd-jest-demo.git"
  },
  "author": "Daniel Maslowski",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/orangecms/antd-jest-demo/issues"
  },
  "homepage": "https://github.com/orangecms/antd-jest-demo#readme",
  "dependencies": {
    "@kadira/storybook": "^2.34.0",
    "antd": "^2.5.2",
    "emoji-generator": "^1.1.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-antd": "^0.5.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "jasmine-expect": "^3.0.1",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.13.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
